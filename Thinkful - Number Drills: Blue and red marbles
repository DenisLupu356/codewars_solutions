#https://www.codewars.com/kata/5862f663b4e9d6f12b00003b/train/python
def guess_blue(blue_start, red_start, blue_pulled, red_pulled):
    total_start = blue_start + red_start
    total_remain = total_start - (blue_pulled + red_pulled)
    blue_remain = blue_start - blue_pulled
    return blue_remain / total_remain
