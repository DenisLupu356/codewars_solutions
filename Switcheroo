#https://www.codewars.com/kata/57f759bb664021a30300007d/train/python
def switcheroo(string):
    result = ""
    for el in string:
        if el == "a":
            result += "b"
        elif el == "b":
            result += "a"
        else:
            result += el
    return result
#2 case:
def vowel_2_index(string):
    vowels = "aouieAOUIE"
    return "".join([str(i+1) if el in vowels else el for i,el in enumerate(string) ])
